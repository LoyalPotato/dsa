{
  "compilerOptions": {
    "outDir": "dist",
    "allowJs": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "target": "es6",
    "module": "commonjs",
    "baseUrl": ".",
    "rootDir": "."
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
